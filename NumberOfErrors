fsol = open("Solpg1.txt","r")
fpg = open("Pg1.txt","r")

c=0
for i in fpg:
    c+=1

fpg.close()
fpg= open("Pg1.txt","r")

count=0
for i in range(c):
    s1=fsol.readline()
    s2=fpg.readline()
    l=len(s1)
    if(len(s1)>len(s2)):
        l=len(s2)
        count+=len(s1)-len(s2)
    
        
    for i in range(l):
        if(s1[i]!=s2[i]):
            count+=1
            
print(count)
